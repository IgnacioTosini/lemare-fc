{
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    }
  ],
  "headers": [
    {
      "source": "(.*\\.(js|css|png|jpg|jpeg|webp|svg|gif|ico|woff|woff2|ttf|otf|eot))$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}